OBJS = analyzer.cmo interpret.cmo ast.cmi sast.cmo pumpkin.cmo

pmkn : $(OBJS)
	ocamlc -o pmkn $(OBJS)

scanner.ml : scanner.mll
	ocamllex scanner.mll

parser.ml parser.mli : parser.mly
	ocamlyacc parser.mly

%.cmo : %.ml
	ocamlc -w A -c $<

%.cmi : %.mli
	ocamlc -w A -c $<

.PHONY : clean
clean :
	rm -f pmkn parser.ml parser.mli scanner.ml \
	*.cmo *.cmi *.out *.diff

# Generated by ocamldep *.ml *.mli
analyzer.cmo : sast.cmo ast.cmi
analyzer.cmx : sast.cmx ast.cmi
interpret.cmo : sast.cmo ast.cmi
interpret.cmx : sast.cmx ast.cmi
pumpkin.cmo : interpret.cmo analyzer.cmo
pumpkin.cmx : interpret.cmx analyzer.cmx
sast.cmo : ast.cmi
sast.cmx : ast.cmi
ast.cmi :
